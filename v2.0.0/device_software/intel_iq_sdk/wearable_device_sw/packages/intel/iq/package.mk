# This package contains Intel proprietary IQ code

# Add path to source code
src-y += $(T)/packages/intel/iq/protobuf/
obj-y += $(T)/packages/intel/iq/protobuf/
obj-y += $(T)/packages/intel/iq/notifications/
obj-y += $(T)/packages/intel/iq/itm/
obj-y += $(T)/packages/intel/iq/sensing/
obj-y += $(T)/packages/intel/iq/events/
obj-y += $(T)/packages/intel/iq/device/
obj-y += $(T)/packages/intel/iq/body/
obj-y += $(T)/packages/intel/iq/fota/
obj-y += $(T)/packages/intel/iq/init/

# Add include paths

subdir-cflags-y += -I$(T)/packages/intel/iq/include/

# Protobuf autogenerated headers and nanopb headers
subdir-cflags-$(CONFIG_COMMON_IQ) += \
	-I$(OUT)/packages/intel/iq/protobuf \
	-I$(T)/external/nanopb
